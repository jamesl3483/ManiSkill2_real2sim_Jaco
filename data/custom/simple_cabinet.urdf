    <child link="cabinet_door"/>
        <origin xyz="0.30500 0.29750 0.42250" rpy="0 0 0"/>
    <axis xyz="0 0 1"/> <!-- Rotate around vertical Z-axis -->
    <limit lower="0" upper="3.14" effort="0.0" velocity="0.0"/>
    </joint>

